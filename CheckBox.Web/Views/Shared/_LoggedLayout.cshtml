<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="icon" href="/imgs/newlogo.png" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <div class="mt-3">
        <header>
            <nav class="nav nav-pills flex-column flex-sm-row justify-content-end bg-transparent">
                <a class="pt-2  flex-sm-justified text-sm-center nav-link text-muted"> <!--asp-action="Profile" asp-controller="User"--->Disponivel em breve</a>
                <a class="pt-2 mr-2 flex-sm-justified btn btn-outline-light  text-sm-center nav-link" asp-action="Index" asp-controller="Note">Notes</a>
                <a class="pt-2 mr-2 flex-sm-justified btn btn-outline-danger  text-sm-center nav-link" asp-action="Logout" asp-controller="Auth">Logout</a>
            </nav>
        </header>
        <main role="main">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
